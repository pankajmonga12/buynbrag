					$("#alertPopup").dialog('close');
				});
					}
					else if (data=='pw_mismatch')
					{$("#alertPopup").dialog({
					width:605,
					height:293,
					modal:true
				});
				$('#alert_text').html('The 2 passwords you entered, do not match!');
				$("#alertclose").click(function(){
